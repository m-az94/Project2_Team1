{{!-- Create Patient --}}
<form class="body">
    <label>Name: </label>
    <input type="text" class="form-control" placeholder="Enter patient's name here" id="p-name"><br>
    <label>Phone Number: </label>
    <input type="text" class="form-control" placeholder="Enter patient's phone number here" id="p-phone"><br>
    <label>City: </label>
    <input type="text" class="form-control" placeholder="Enter patient's address here" id="p-city"><br>
    <label>Email: </label>
    <input type="text" class="form-control" placeholder="Enter patient's email here" id="p-email"><br>
    <label>Health Card Number: </label>
    <input type="text" class="form-control" placeholder="Enter patient's healthcard number here" id="p-healthcard"><br>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>

